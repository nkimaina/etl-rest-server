{
    "name": "patintChangeStatusTrackerDataSetbase",
    "version": "1.0",
    "tag": "",
    "description": "",
    "uses": [],
    "sources": [
        {
            "table": "etl.hiv_monthly_report_dataset_v1_2",
            "alias": "fhs"
        },
        {
            "table": "amrs.location",
            "alias": "l",
            "join": {
                "type": "INNER",
                "joinCondition": "l.location_id = fhs.location_id"
            }
        }
    ],
    "columns": [
        {
            "type": "simple_column",
            "alias": "person_id",
            "column": "fhs.person_id"
        },
        {
            "type": "derived_column",
            "alias": "total_patients",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when fhs.person_id is not null then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "active_return",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when status='active' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "active_return_to_active_return",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='active' and status='active' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "active_return_to_LTFU",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='active' and status='ltfu' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "active_return_to_dead",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='active' and status='dead' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "active_return_to_transfer_out",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='active' and status='transfer_out' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "LTFU",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when status='ltfu' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "LTFU_to_LTFU",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='ltfu' and status='ltfu' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "LTFU_to_active_return",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='ltfu' and status='active' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "LTFU_to_dead",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='ltfu' and status='dead' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "LTFU_to_transfer_out",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='ltfu' and status='transfer_out' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "deaths",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when status='dead' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "dead_to_LTFU",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='dead' and status='ltfu' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "dead_to_active_return",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='dead' and status='active' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "dead_to_dead",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='dead' and status='dead' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "dead_to_transfer_out",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='dead' and status='transfer_out' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "transfer_out_patients",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when status='transfer_out' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "transfer_out_to_transfer_out",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='transfer_out' and status='transfer_out' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "transfer_out_to_active_return",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='transfer_out' and status='active' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "transfer_out_to_LTFU",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='transfer_out' and status='ltfu' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "transfer_out_to_dead",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status='transfer_out' and status='dead' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "none_to_active",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status is null and status='active' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "none_to_other",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "case when prev_status is null and status <> 'active' then 1 else null end"
            }
        },
        {
            "type": "derived_column",
            "alias": "reporting_month",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "date_format(fhs.endDate, '%m/%Y') "
            }
        },
        {
            "type": "simple_column",
            "alias": "reporting_date",
            "column": "fhs.endDate"
        },
        {
            "type": "simple_column",
            "alias": "location_uuid",
            "column": "fhs.location_uuid"
        },
        {
            "type": "simple_column",
            "alias": "location_id",
            "column": "fhs.location_id"
        }
    ],
    "filters": {
        "conditionJoinOperator": "and",
        "conditions": [
            {
                "filterType": "tableColumns",
                "conditionExpression": "endDate >= ?",
                "parameterName": "startDate"
            },
            {
                "filterType": "tableColumns",
                "conditionExpression": "endDate <= ?",
                "parameterName": "endDate"
            },
            {
                "filterType": "tableColumns",
                "conditionExpression": "fhs.location_id in ?",
                "parameterName": "locations"
            }
        ]
    }
}